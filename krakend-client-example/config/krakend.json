{
    "version": 3,
    "plugin": {
      "pattern": ".so",
      "folder": "./"
    },
    "endpoints": [
      {
        "endpoint": "/test/{id}",
        "backend": [
          {
            "host": [
              "http://localhost:8080"
            ],
            "url_pattern": "/__debug/{id}",
            "extra_config": {
              "plugin/http-client": {
                "name": "krakend-client-example",
                "krakend-client-example": {
                  "path": "/__debug/hijack-me"
                }
              }
            }
          }
        ]
      }
    ]
  }